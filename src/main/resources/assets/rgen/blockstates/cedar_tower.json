{
  "multipart": [
      {   "when": { "up": "true" },
          "apply": { "model": "rgen:wood/cedar/cedar_tower" }
      },
      {   "when": { "north": "true" },
          "apply": { "model": "rgen:wood/cedar/cedar_tower_side", "uvlock": true }
      },
      {   "when": { "east": "true" },
          "apply": { "model": "rgen:wood/cedar/cedar_tower_side", "y": 90, "uvlock": true }
      },
      {   "when": { "south": "true" },
          "apply": { "model": "rgen:wood/cedar/cedar_tower_side", "y": 180, "uvlock": true }
      },
      {   "when": { "west": "true" },
          "apply": { "model": "rgen:wood/cedar/cedar_tower_side", "y": 270, "uvlock": true }
      }
  ]
}
